{
	"manifest_version": 2,
	"name": "Bring Twitter Bird Back",
	"version": "1.2.1",
  
	"description": "Replaces the new Twitter logo with the old one.",
  
	"icons": {
	  "48": "icons/icon.png",
	  "96": "icons/icon@2x.png"
	},
  
	"content_scripts": [
	  {
		"matches": ["*://*.twitter.com/*"],
		"js": ["bring-twitter-bird-back.js"]
	  }
	],

	"web_accessible_resources": [
		"icons/favicon.ico",
		"icons/favicon-notification.ico",
		"icons/icon.png",
		"icons/icon@2x.png"
	],
	"browser_specific_settings": {
		"gecko": {
		  "id": "{1af1a448-95f4-46c6-b093-078ea1023f81}",
		  "update_url": "https://github.com/SauceyRed/bring-twitter-bird-back/blob/main/updates.json"
		}
	}
}